#include<iostream>
using namespace std;
int main()
{
	int n;
	int arr[50],num;
	cout<<"Enter the size of array : \n";
	cin>>n;
	cout<<"Enter element in array : \n";
	for(int i=0;i<n;i++)
	{
		cin>>arr[i];
	}
	cout<<"Enter the number to search : \n";
	cin>>num;
	for(int i=0;i<n;i++)
	{
		if(num!=arr[i])
		{
		    cout<<"Element not found.\n";
		}
		else
		{
			cout<<"Element found at "<<i<<" position. \n";
		}
	}
	return 0;
	
}
