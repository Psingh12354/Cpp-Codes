#include<iostream>
using namespace std;
int main()
{
    int i,j;
    int a[100],n;
    cout<<"Enter the size of array : \n";
    cin>>n;
    cout<<"Enter array element : \n";
    for(i=0;i<n;i++)
    {
        cin>>a[i];
    }
    cout<<"Array element is : \n";
    for(int i=0;i<n;i++)
    {
        cout<<*(a+i)<<" ";
    }
    return 0;
}
